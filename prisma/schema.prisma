datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Member {
  id          String  @id @default(uuid())
  displayName String
  email       String
  profiles    Profile @relation(fields: [profileId], references: [id])
  profileId   Int
}

model Profile {
  id       Int      @id
  Member   Member[]
  username String
  htmlUrl  String
}
